{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Angular in Chrome",
      "type": "pwa-chrome",
      "request": "launch",
      "url": "http://localhost:4200",
      "webRoot": "${workspaceFolder}",
      "sourceMaps": true,
      "trace": true,
      "skipFiles": ["node_modules/**/*.js"]
    }
  ]
}
